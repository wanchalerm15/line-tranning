<%- include('../layouts/header.ejs') %>

<div class="container">
    <div class="card">
        <h4 class="card-header"><%= message %> </h4>
        <div class="card-body" id="orm">
            <table class="table">
                <thead class="thead-light">
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Message</th>
                        <th>Date</th>
                    </tr>
                </thead>
                <tbody>
                    <tr v-for="item in items">
                        <td>{{ item.id }}</td>
                        <td>{{ item.name }}</td>
                        <td>{{ item.message }}</td>
                        <td>{{ item.date }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<script src="/thee.js" defer></script>
<%- include('../layouts/footer.ejs') %>